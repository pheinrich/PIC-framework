                      00128 ;; ----------------------------------------------
                      00129 ;;  void Clock.waitMS()
                      00130 ;;
                      00131 ;;  Enters a busy loop (suspends normal execution) until the tick count equals
                      00132 ;;  a specified alarm value, settable by updating Clock.Alarm directly or by
                      00133 ;;  using the WaitMS macro.  On entry, this routine expects the alarm reg-
                      00134 ;;  isters to hold the desired delay, in milliseconds.
                      00135 ;;
                      00136 ;;  Note that interrupts must not be disabled when this routine runs, since it
                      00137 ;;  depends on Clock.Ticks being volatile and updated asynchronously by the
                      00138 ;;  interrupt service routine.
                      00139 ;;
000034                00140 Clock.waitMS:
                      00141    ; Add the alarm value to the current tick count, creating a "target" tick count
                      00142    ; to match.  Once the actual tick count reaches the target value, the delay is
                      00143    ; complete.
000034 50??           00144    movf     Clock.Ticks, W
000036 26??           00145    addwf    Clock.Alarm       ; first byte (LSB)
                      00146 
000038 50??           00147    movf     Clock.Ticks + 1, W
00003A 22??           00148    addwfc   Clock.Alarm + 1   ; second byte
                      00149 
